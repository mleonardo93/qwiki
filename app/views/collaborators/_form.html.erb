<%= form_for [wiki, Collaborator.new] do |f| %>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :temp_user_email %>
    <br>
    <%= f.label :nickname %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.submit "Add collaborator", class: "btn btn-success" %>
  </div>
<% end %>